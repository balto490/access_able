<p id="notice"><%= notice %></p>

<h1>Geocodes</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @geocodes.each do |geocode| %>
      <tr>
        <td><%= geocode.address %></td>
        <td><%= geocode.latitude %></td>
        <td><%= geocode.longitude %></td>
        <td><%= link_to 'Show', geocode %></td>
        <td><%= link_to 'Edit', edit_geocode_path(geocode) %></td>
        <td><%= link_to 'Destroy', geocode, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Geocode', new_geocode_path %>
